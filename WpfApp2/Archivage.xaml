            <ComboBox x:Name="Employee_filtrage" materialdesign:HintAssist.Hint="Employée" Margin="439,0,261,295" VerticalAlignment="Bottom" d:LayoutOverrides="HorizontalAlignment"/>
            <ComboBox x:Name="Remboursable_filtrage" materialdesign:HintAssist.Hint="Remboursable" Margin="80,0,620,201" VerticalAlignment="Bottom"/>


            <Border CornerRadius="20" Width="100" Height="32" VerticalAlignment="Bottom" HorizontalAlignment="Left" Background="#FF1FAB89" Margin="890,0,0,28">
                <Button x:Name="Confirmer_Flitrage"  VerticalAlignment="Bottom" Width="100" Background="Transparent" Content="Confirmer" Foreground="White" FontWeight="Bold" Margin="0,0,0,0"  BorderBrush="{x:Null}" Click="Confirmer_Filtrage_Click" ></Button>
            </Border>
            <Border CornerRadius="20" Width="100" Height="32" VerticalAlignment="Bottom" HorizontalAlignment="Left" Background="#FF1FAB89" Margin="746,0,0,28">
